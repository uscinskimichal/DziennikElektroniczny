<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="520.0" prefWidth="874.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Notes.CheckNotesIPutWindowController">
   <children>
      <ComboBox fx:id="classesBox" layoutX="22.0" layoutY="46.0" onAction="#getSubjectsOfClass" prefHeight="43.0" prefWidth="170.0" promptText="Wybierz klasę" />
      <ComboBox fx:id="subjectsBox" layoutX="213.0" layoutY="46.0" onAction="#getMembersOfClass" prefHeight="43.0" prefWidth="189.0" promptText="Wybierz przedmiot" />
      <ComboBox fx:id="membersBox" layoutX="409.0" layoutY="46.0" onAction="#showNotesIPut" prefHeight="43.0" prefWidth="215.0" promptText="Wybierz ucznia" />
      <TableView fx:id="tableView" layoutX="33.0" layoutY="164.0" prefHeight="233.0" prefWidth="531.0">
        <columns>
          <TableColumn fx:id="columnData" prefWidth="135.0" text="Data" />
          <TableColumn fx:id="columnValue" prefWidth="54.0" text="Wartość" />
            <TableColumn fx:id="columnType" prefWidth="140.0" text="Typ" />
            <TableColumn fx:id="columnComment" prefWidth="189.0" text="Uwagi" />
        </columns>
      </TableView>
      <Button layoutX="44.0" layoutY="462.0" mnemonicParsing="false" onAction="#backToMenu" text="back" />
      <Button layoutX="733.0" layoutY="345.0" mnemonicParsing="false" onAction="#editNote" text="Edytuj ocenę" />
      <Button layoutX="506.0" layoutY="132.0" mnemonicParsing="false" onAction="#refreshNotes" text="reresh" />
   </children>
</AnchorPane>
