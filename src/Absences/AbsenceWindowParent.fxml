                <TableColumn fx:id="dataColumn" minWidth="0.0" prefWidth="129.0" text="Data" />
                <TableColumn fx:id="statusColumn" minWidth="0.0" prefWidth="130.0" text="Usprawiedliwiona" />
            </columns>
        </TableView>
        <Label layoutX="25.0" layoutY="14.0" prefHeight="49.0" prefWidth="273.0" text="Twoje nieobecności:" />
        <Button fx:id="backButton" layoutX="25.0" layoutY="321.0" mnemonicParsing="false" onAction="#backToMenu" text="Powrót" />
        <ComboBox fx:id="comboBox" layoutX="25.0" layoutY="21.0" onAction="#setDataToDisplay" prefHeight="36.0" prefWidth="240.0" promptText="Wybierz dziecko" />
    </children>
</AnchorPane>
